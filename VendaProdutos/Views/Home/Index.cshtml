<script>
    function obterAlturaLargura(imagem) {

        // Obter altura e largura renderizadas da imagem
        var alturaRenderizada = imagem.clientHeight;
        var larguraRenderizada = imagem.clientWidth;

        // Adicionar os atributos height e width à imagem
        imagem.setAttribute('height', alturaRenderizada);
        imagem.setAttribute('width', larguraRenderizada);

        
    }
  
</script>
<script>
    function verificarStatusImagem(idImagem) {
        var imagemCategoriaHome = document.getElementById(idImagem);

       

        imagemCategoriaHome.addEventListener('error', function () {
            // Erro ao carregar a imagem, adicione o estilo display: none
            var divPai = imagemCategoriaHome.closest('.col-6.col-md-3');
            if (divPai) {
                divPai.remove();
            }
        });
    }

    // Chame a função para cada imagem, passando o ID correspondente
    verificarStatusImagem('categoriaHome1');
    verificarStatusImagem('categoriaHome2');
    verificarStatusImagem('categoriaHome3');
    verificarStatusImagem('categoriaHome4');
    verificarStatusImagem('categoriaHome5');
    verificarStatusImagem('categoriaHome6');
    verificarStatusImagem('categoriaHome7');
    verificarStatusImagem('categoriaHome8');

</script>



